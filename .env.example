# Environment Variables Example
# Copy this file to .env.local and fill in the values

# Houjin Bangou API (法人番号システムWeb-API)
# Get your API key from: https://www.invoice-kohyo.nta.go.jp/secure/reqApplicationCode.do
KEI_CORPORATE_API_ID="your_api_key_here"

# Development Mode
NEXT_PUBLIC_DEV_MODE="false"
ENABLE_DEV_MOCK_AUTH="false"

# Site URL
NEXT_PUBLIC_SITE_URL="https://package-lab.com"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key"
NEXT_PUBLIC_SUPABASE_URL="your_supabase_url"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"
SUPABASE_SENDER_EMAIL="info@package-lab.com"
SUPABASE_SENDER_NAME="Epackage Lab"
SUPABASE_SMTP_HOST="sv12515.xserver.jp"
SUPABASE_SMTP_PASSWORD="your_smtp_password"
SUPABASE_SMTP_PORT="587"
SUPABASE_SMTP_USER="info@package-lab.com"

# Google OAuth
NEXT_PUBLIC_GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"
NEXT_PUBLIC_GOOGLE_REDIRECT_URI="https://package-lab.com"
GOOGLE_DRIVE_UPLOAD_FOLDER_ID="your_drive_folder_id"
GOOGLE_DRIVE_CORRECTION_FOLDER_ID="your_drive_folder_id"
