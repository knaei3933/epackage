{
  "name": "epackage-lab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production next build",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "test:performance": "npm run build && npm run lighthouse",
    "test:lighthouse": "npm run build && start-server-and-test start http://localhost:3000 lighthouse",
    "cache:clear": "node scripts/clear-cache.js",
    "cache:verify": "node scripts/verify-cache-clear.js",
    "dev:fresh": "npm run cache:clear && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='src.*\\.(test|spec)\\.[jt]s(x?)$'",
    "test:integration": "jest --testPathPattern='tests.*\\.(test|spec)\\.[jt]s(x?)$'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:multi-quantity": "jest --testPathPattern='multi-quantity'",
    "test:accessibility": "jest --testPathPattern='accessibility'",
    "test:api": "jest --testPathPattern='api'",
    "test:all": "npm run lint && npm run test:unit && npm run test:e2e && npm run test:performance",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.84.0",
    "@types/jspdf": "^1.3.3",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.554.0",
    "next": "^16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "web-vitals": "^4.2.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@mswjs/data": "^0.16.2",
    "@next/bundle-analyzer": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.11.0",
    "babel-plugin-react-compiler": "1.0.0",
    "chrome-launcher": "^1.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-webextension-mock": "^4.0.0",
    "lighthouse": "^13.0.1",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5",
    "vitest": "^4.0.13"
  }
}
